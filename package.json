{
  "name": "elastic-tabstops",
  "main": "./lib/elastic-tabstops",
  "version": "1.2.0",
  "description": "Elastic Tabstops for Atom",
  "keywords": [
    "Elastic Tabstops",
    "tab"
  ],
  "repository": "https://github.com/hax/atom-elastic-tabstops",
  "license": "MIT",
  "engines": {
    "atom": ">=1.19.0"
  },
  "dependencies": {
    "atom-ts-transpiler": "^1.2.4",
    "typescript": "^2.6.1"
  },
  "devDependencies": {
    "@types/atom": "^1.22.1"
  },
  "atomTranspilers": [
    {
      "transpiler": "atom-ts-transpiler",
      "glob": "{!(node_modules)/**/,}*.ts?(x)",
      "options": {
        "compilerOptions": { },
        "cacheKeyFiles": [ "tsconfig.json" ],
        "verbose": false
      }
    }
  ]
}
